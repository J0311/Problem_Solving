public class NumberStuff {

	public static void main(String[] args) {
		
		/**
		 * Using for loop, print the result of multiplication of 
		 * all odd numbers from 1 to 20 and the result of sum of 
		 * all even numbers from 1 to 100.
		 */
		
		int even = 0, odd = 1;
		
		// loop for detecting odd numbers:
		// iterate by += 2 to stay on odd numbers
		
		for (int i = 1; i <= 20; i += 2) {
			odd = odd * i;
		}
		
		for (int i = 2; i <= 100; i += 2) {
			even = even + i;
		}
		
		System.out.println("Odd: " + odd );
		System.out.println("Even: "+ even);
		
	
	}
}
